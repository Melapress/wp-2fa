{
  "name": "wp-2fa",
  "version": "2.0.0",
  "description": "Easy to use two-factor authentication for your WordPress logins.",
  "main": "babel.config.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "vendor:install": "sh bin/install-dependencies.sh",
    "convert:free": "gulp convert-to-free-edition && gulp remove-annotations",
    "release": "npm run prod && gulp remove-free-only-code && gulp replace-plugin-name && gulp replace-latest-version-numbers && gulp remove-annotations && gulp zip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WPWhiteSecurity/wp-2fa-premium.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WPWhiteSecurity/wp-2fa-premium/issues"
  },
  "homepage": "https://github.com/WPWhiteSecurity/wp-2fa-premium#readme",
  "devDependencies": {
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "copy-webpack-plugin": "^5.1.2",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-remove-lines": "^0.1.0",
    "gulp-replace": "^1.1.3",
    "gulp-sort": "^2.0.0",
    "gulp-strip-code": "^0.1.4",
    "gulp-wp-pot": "^2.5.0",
    "gulp-zip": "^5.1.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "laravel-mix": "^6.0.19",
    "laravel-mix-compress-images": "^1.0.4",
    "laravel-mix-imagemin": "^1.0.3",
    "postcss": "^8.3.0",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "resolve-url-loader": "^3.1.3",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "shelljs": "^0.8.4"
  },
  "dependencies": {
    "micromodal": "^0.4.6"
  }
}
